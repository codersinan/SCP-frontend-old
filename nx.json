{
  "npmScope": "scp-frontend",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "dashboard": {
      "tags": ["scp", "dashboard"]
    },
    "dashboard-e2e": {
      "tags": [],
      "implicitDependencies": ["dashboard"]
    },
    "api-interfaces-identity": {
      "tags": ["scp","api-services", "identity"]
    },
    "translate-transloco": {
      "tags": ["scp", "translate", "transloco"]
    },
    "core-angular-core": {
      "tags": ["scp", "core"]
    }
  }
}
