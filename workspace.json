{
  "version": 1,
  "projects": {
    "dashboard": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/dashboard",
      "sourceRoot": "apps/dashboard/src",
      "prefix": "dashboard",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/dashboard/browser",
            "index": "apps/dashboard/src/index.html",
            "main": "apps/dashboard/src/main.ts",
            "polyfills": "apps/dashboard/src/polyfills.ts",
            "tsConfig": "apps/dashboard/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/dashboard/src/favicon.ico",
              "apps/dashboard/src/assets",
              "apps/dashboard/src/manifest.webmanifest",
              {
                "glob": "**/*",
                "input": "node_modules/@ant-design/icons-angular/src/inline-svg/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "apps/dashboard/src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "flat"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/dashboard/src/environments/environment.ts",
                  "with": "apps/dashboard/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "apps/dashboard/ngsw-config.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "dashboard:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "dashboard:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "tsConfig": [
              "apps/dashboard/tsconfig.app.json",
              "apps/dashboard/tsconfig.spec.json",
              "apps/dashboard/tsconfig.server.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/dashboard/**/*"],
            "linter": "eslint"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/dashboard/jest.config.js",
            "tsConfig": "apps/dashboard/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/dashboard/src/test-setup.ts"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/dashboard/server",
            "main": "apps/dashboard/src/main.server.ts",
            "tsConfig": "apps/dashboard/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "apps/dashboard/src/environments/environment.ts",
                  "with": "apps/dashboard/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "app-shell": {
          "builder": "@angular-devkit/build-angular:app-shell",
          "options": {
            "browserTarget": "dashboard:build",
            "serverTarget": "dashboard:server",
            "route": "shell"
          },
          "configurations": {
            "production": {
              "browserTarget": "dashboard:build:production",
              "serverTarget": "dashboard:server:production"
            }
          }
        }
      }
    },
    "dashboard-e2e": {
      "root": "apps/dashboard-e2e",
      "sourceRoot": "apps/dashboard-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dashboard-e2e/cypress.json",
            "tsConfig": "apps/dashboard-e2e/tsconfig.e2e.json",
            "devServerTarget": "dashboard:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dashboard:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/dashboard-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/dashboard-e2e/**/*"]
          }
        }
      }
    },
    "api-interfaces-identity": {
      "root": "libs/api-interfaces/identity",
      "sourceRoot": "libs/api-interfaces/identity/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/api-interfaces/identity/tsconfig.lib.json",
              "libs/api-interfaces/identity/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/api-interfaces/identity/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/api-interfaces/identity/jest.config.js",
            "tsConfig": "libs/api-interfaces/identity/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "translate-transloco": {
      "projectType": "library",
      "root": "libs/translate/transloco",
      "sourceRoot": "libs/translate/transloco/src",
      "prefix": "translate",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "tsConfig": [
              "libs/translate/transloco/tsconfig.lib.json",
              "libs/translate/transloco/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/translate/transloco/**/*"],
            "linter": "eslint"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/translate/transloco/jest.config.js",
            "tsConfig": "libs/translate/transloco/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/translate/transloco/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "core-angular-core": {
      "projectType": "library",
      "root": "libs/core/angular/core",
      "sourceRoot": "libs/core/angular/core/src",
      "prefix": "core",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "tsConfig": [
              "libs/core/angular/core/tsconfig.lib.json",
              "libs/core/angular/core/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core/angular/core/**/*"],
            "linter": "eslint"
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core/angular/core/jest.config.js",
            "tsConfig": "libs/core/angular/core/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/core/angular/core/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "style": "scss"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest",
      "style": "scss",
      "linter": "eslint"
    }
  },
  "defaultProject": "dashboard"
}
